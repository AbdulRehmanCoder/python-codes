#creating power function without using power operator
def power(base,exponent):
    power = 1
    i = 0
    while i < exponent:
        power *= base 
        i += 1
    return power


print(power(10,5))

#creating power function using power operator
def power(base,exponent):
    power = base ** exponent
    return power


print(power(2,5))
